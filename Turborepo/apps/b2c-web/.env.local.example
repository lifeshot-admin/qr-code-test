# Bubble.io Data API
BUBBLE_API_BASE_URL=https://api.lifeshot.me
BUBBLE_API_TOKEN=[여기에_복사한_프라이빗_키_입력]
BUBBLE_USE_VERSION_TEST=true

# Backend API (실제 앱 백엔드)
NEXT_PUBLIC_API_BASE_URL=https://api.lifeshot.me

# NextAuth
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=[임의의_긴_문자열_생성]

# Kakao OAuth
KAKAO_CLIENT_ID=[카카오_REST_API_키]
KAKAO_CLIENT_SECRET=[카카오_시크릿_키]

# Google OAuth (선택사항)
GOOGLE_CLIENT_ID=[구글_클라이언트_ID]
GOOGLE_CLIENT_SECRET=[구글_시크릿]
